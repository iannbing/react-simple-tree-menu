{
  "name": "react-simple-tree-menu",
  "version": "1.1.9",
  "description": "A simple React tree menu component",
  "keywords": [
    "react",
    "tree",
    "menu",
    "react-component",
    "tree menu",
    "render-props",
    "control-props",
    "downshift"
  ],
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "jest",
    "test:ci": "jest --ci --runInBand --reporters=jest-junit",
    "build:dev": "webpack --config webpack.dev.js && npm run sass-build",
    "build": "webpack --config webpack.prod.js && npm run sass-build",
    "style:compile-sass": "node-sass-chokidar --source-map true src/sass/ -o dist",
    "style:autoprefixer": "postcss dist/*.css --use autoprefixer -d dist",
    "sass-build": "npm-run-all -p style:*",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook && node .storybook/deploy-helper.js",
    "deploy-storybook": "storybook-to-ghpages -e storybook-static",
    "publish:beta": "npm publish --tag beta",
    "publish": "npm publish",
    "bump-version": "npm version",
    "bump-version:beta": "npm version prerelease --preid=rc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iannbing/react-simple-tree-menu.git"
  },
  "author": "Huang-Ming Chang <huang.m.chang@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iannbing/react-simple-tree-menu/issues"
  },
  "homepage": "https://github.com/iannbing/react-simple-tree-menu#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@storybook/storybook-deployer": "^2.8.1",
    "@storybook/theming": "^5.2.8",
    "@types/classnames": "^2.2.8",
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "autoprefixer": "^9.6.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.11.2",
    "bootstrap": "^4.3.1",
    "chokidar-cli": "^1.2.2",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "escape-string-regexp": "^2.0.0",
    "fs-extra": "^8.0.1",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "prettier": "^1.15.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-test-renderer": "^16.7.0",
    "reactstrap": "^8.0.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.2.2",
    "webpack": "^4.26.1",
