    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  },
  "peerDependencies": {
    "react": ">=16.6.3",
    "react-dom": ">=16.6.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "fast-memoize": "^2.5.1",
    "is-empty": "^1.2.0",
    "tiny-debounce": "^0.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
